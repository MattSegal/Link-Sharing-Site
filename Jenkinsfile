pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                sh 'echo "Clone!!!"'
            }
        }
        stage('Build') {
            steps {
                sh 'echo "Build"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Test"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploy"'
            }
        }
    }
}
